<div id="modal-window" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <button class="close" data-dismiss="modal">x</button>
    <div class="modal-content">
      <h1>
        User Report
      </h1>
      <div class="modal-body">
        <div class="container">
          <table id="data-table" class="table table-striped table-bordered" style="width: 800px;">
            <thead>
            <th>Project Name</th>
            <th>Score</th>
            <th>Comment</th>
            </thead>
            <tbody>
            <% Evaluation.where(user_id: current_user.id).where(course_id: Course.find_by(id: params[:course_id]).id).each do |eval| %>
              <tr>
                <td><%= eval.project_id.project.name %></td>
                <td><%= eval.score %></td>
                <td><%= eval.comment %></td>
              </tr>
            <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>


