<h1>Sections</h1>
<div class="container">
  <table id="data-table" class="table table-striped table-bordered" style="width: 800px;">
    <thead>
    <th>Section ID</th>
    <th>Subject</th>
    <th>Number</th>
    <th>Course Name</th>
    <th></th>
    </thead>
    <tbody>
    <% Course.where(user_id: current_user.id).each do |course| %>
      <tr>
        <td><%= course.course_id %></td>
        <td><%= course.subject %></td>
        <td><%= course.number %></td>
        <td><%= course.name %></td>
        <td><%= link_to 'Select', aman_path(:course_id => course.course_id) %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

