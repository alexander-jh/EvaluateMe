<h1>Group Management</h1>
<div class="container">
  <table id="example" class="table table-striped table-bordered" style="width: 1000px;">
    <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Average Score</th>
    <th>Make Admin</th>
    <th>Action</th>
    </thead>
    <tbody>
    <% Ingroup.all.each do |user| %>
      <tr>
        <td><%=user.user_id.Fname%></td>
        <td><%=user.user_id.Lname%></td>
        <td><%=user.user_id.email%></td>
        <td>N/A</td>
        <td></td>
        <td><%= link_to 'Delete', user, method: destroy %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

<%= link_to 'Add User', new_ingroup_path, {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#modal-window'}  %>
