<h1>User Report for Section</h1>
<div class="container">
  <table id="data-table" class="table table-striped table-bordered" style="width: 800px;">
    <thead>
      <th>User ID</th>
      <th>Email</th>
    </thead>
    <tbody>
    <% User.all.each do |user| %>
      <tr>
        <td><%= user.object_id %></td>
        <td><%= user.email %></td>
        <td></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

