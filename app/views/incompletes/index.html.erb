<p id="notice"><%= notice %></p>

<h1>Incompletes</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incompletes.each do |incomplete| %>
      <tr>
        <td><%= incomplete.project_id %></td>
        <td><%= incomplete.user_id %></td>
        <td><%= link_to 'Show', incomplete %></td>
        <td><%= link_to 'Edit', edit_incomplete_path(incomplete) %></td>
        <td><%= link_to 'Destroy', incomplete, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incomplete', new_incomplete_path %>
